{% extends 'base.html.twig' %}

{% block title %}Dlc{% endblock %}

{% block body %}
    <h1>Dlc</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ dlc.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ dlc.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ dlc.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('dlc_index') }}">back to list</a>

    <a href="{{ path('dlc_edit', {'id': dlc.id}) }}">edit</a>

    {{ include('dlc/_delete_form.html.twig') }}
{% endblock %}
